<body class="sb-nav-fixed">
  <!-- main content -->
  <div id="layoutSidenav_content">
    <main>
      <h1>Danh sách các hãng</h1>
      <table id="brand-table" class="hover cell-border order-column">
        <thead>
          <tr>
            <th>Mã hãng</th>
            <th>Tên hãng</th>
            <th>Ngày thêm</th>
          </tr>
        </thead>
        <tbody>
          {{#each brands}}
            <tr>
              <td>{{brand_id}}</td>
              <td>{{brand_name}}</td>
              <td>{{created_date}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </main>
  </div>
